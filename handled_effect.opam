opam-version: "2.0"
maintainer: "Jane Street developers"
authors: ["Jane Street Group, LLC"]
homepage: "https://github.com/janestreet/handled_effect"
bug-reports: "https://github.com/janestreet/handled_effect/issues"
dev-repo: "git+https://github.com/janestreet/handled_effect.git"
doc: "https://ocaml.janestreet.com/ocaml-core/latest/doc/handled_effect/index.html"
license: "MIT"
build: [
  ["dune" "build" "-p" name "-j" jobs]
]
depends: [
  "ocaml" {>= "5.1.0"}
  "base"
  "basement"
  "ppx_template"
  "dune"         {>= "3.17.0"}
]
available: arch != "arm32" & arch != "x86_32"
synopsis: "Handled effects API for OxCaml"
description: "
Handled_effect provides an effect-safe wrapper for algebraic effects
    using Jane Street compiler extensions.
"
